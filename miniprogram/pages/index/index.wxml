<!--index.wxml-->
<view class="container">
  <image class="banner" src="https://metag.oss-cn-hangzhou.aliyuncs.com/miniprogram/music_separation/img/banner.jpg?OSSAccessKeyId=LTAI5tMch8zYHoxEUxLmrF6c&Expires=1714753345&Signature=NLFVN2uuFvnUr2NC6ZKZYf0RPOA%3D" />
  <view class="title">视频提取</view>
  <view class="list">
    <view class="item">
      <van-icon name="live" size="56rpx"/>
      <view class="name">提取视频中音乐</view>
    </view>
    <view class="item">
      <van-icon name="friends" size="56rpx"/>
      <view class="name">提取视频中人声</view>
    </view>
  </view>
  <view class="title">音频提取</view>
  <view class="list">
    <view class="item">
      <van-icon name="audio" size="56rpx"/>
      <view class="name">提取音频中音乐</view>
    </view>
    <view class="item">
      <van-icon name="manager" size="56rpx"/>
      <view class="name">提取音频中人声</view>
    </view>
  </view>
  <view class="title">音乐转谱</view>
  <view class="list single">
    <view class="item">
      <van-icon name="music" size="56rpx"/>
      <view class="name">音乐转五线谱</view>
    </view>
  </view>
  <van-tabbar active="{{ active }}" bind:change="onChange">
    <van-tabbar-item name="home" icon="wap-home-o">首页</van-tabbar-item>
    <van-tabbar-item name="records" icon="records">记录</van-tabbar-item>
    <van-tabbar-item name="music" icon="music-o">会员</van-tabbar-item>
    <van-tabbar-item name="user" icon="user-o">我的</van-tabbar-item>
  </van-tabbar>
</view>
